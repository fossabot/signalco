{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build",
                "$NEXT_PUBLIC_APP_ENV",
                "$NODE_ENV",
                "$ANALYZE"
            ],
            "outputs": [
                ".next/**",
                "dist/**",
                "storybook-static/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "test": {
            "outputs": [
                "coverage/**"
            ],
            "dependsOn": [
                "build"
            ]
        },
        "dev": {
            "cache": false
        },
        "clean": {
            "cache": false
        }
    },
    "remoteCache": {
        "signature": true
    }
}
