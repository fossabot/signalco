{
    "name": "@signalco/ui",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "sideEffects": false,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
