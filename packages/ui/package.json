        "tsup": "6.5.0",
        "typescript": "4.9.3"
    },
    "dependencies": {
        "@mui/system": "5.10.14",
        "@signalco/ui-icons": "*",
        "lucide-react": "0.101.0",
        "next": "13.0.3"
    }
}
