{
    "name": "@signalco/ui",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "sideEffects": false,
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "npm run clean && concurrently \"npm run build:bundle\" \"npm run build:types\"",
        "build:bundle": "rollup -c",
        "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.build.json",
        "dev": "concurrently --kill-others \"npm run dev:bundle\" \"npm run dev:types\"",
        "dev:bundle": "rollup --watch -c",
        "dev:types": "tsc --watch --emitDeclarationOnly --declaration --project tsconfig.build.json",
        "clean": "rimraf dist",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^23.0.4",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@signalco/tsconfig": "workspace:*",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "@types/react-timeago": "4.1.3",
        "concurrently": "^7.6.0",
        "esbuild": "^0.16.4",
        "postcss": "8.4.20",
        "postcss-preset-env": "^7.8.3",
        "resolve-url-loader": "^5.0.0",
        "rimraf": "3.0.2",
        "rollup": "^3.7.2",
        "rollup-plugin-esbuild": "^5.0.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-summary": "^2.0.0",
        "sass": "1.57.0",
        "typescript": "4.9.4"
    },
    "dependencies": {
        "@mui/base": "5.0.0-alpha.110",
        "@mui/joy": "5.0.0-alpha.58",
        "@mui/system": "5.11.0",
        "@signalco/ui-icons": "workspace:*",
        "material-ui-popup-state": "5.0.4",
        "next": "13.0.7",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-timeago": "7.1.0"
    },
    "peerDependencies": {
        "@mui/base": "5.0.0-alpha.110",
        "@mui/joy": "5.0.0-alpha.58",
        "@mui/system": "5.11.0",
        "@signalco/ui-icons": "workspace:*",
        "@types/react": "18.0.26",
        "material-ui-popup-state": "5.0.4",
        "next": "13.0.7",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "styled-components": "5.3.6"
    }
}
