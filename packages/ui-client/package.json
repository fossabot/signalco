{
    "name": "@signalco/ui-client",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "sideEffects": false,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "type": "module",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm --treeshake --dts --external react",
