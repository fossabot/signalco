    },
    "devDependencies": {
        "@signalco/tsconfig": "*",
        "tsup": "6.5.0",
        "typescript": "4.9.3",
        "react": "18.2.0",
        "@types/react": "18.0.25",
        "@types/react-dom": "18.0.9"
    },
    "dependencies": {
        "@signalco/ui-icons": "*",
        "@signalco/ui": "*",
        "react-dom": "18.2.0",
        "@mui/system": "5.10.14",
        "@mui/joy": "5.0.0-alpha.54",
        "@mui/base": "5.0.0-alpha.106",
        "material-ui-popup-state": "4.1.0",
        "@storybook/react": "6.5.13",
        "next": "13.0.4"
    }
}
